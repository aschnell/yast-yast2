.yast2.menuentry

`ag_ini(
  IniAgent( [ "/usr/share/YaST2/config/y2cc.groups", "/usr/share/YaST2/config/*.y2cc" ],
    $[
      "options" : [ ],
      "comments": [ "^[ \t]*;.*", ";.*", "\\{[^}]*\\}", "^[ \t]*$" ],
      "sections" : [
        $[
        "begin" : [ "^[ \t]*\\[[ \t]*(.*[^ \t])[ \t]*\\][ \t]*", "[%s]" ],
        ],
      ],
      "params" : [
        $[
        "match" : [ "^[ \t]*([^=]*[^ \t=])[ \t]*=[ \t]*(.*[^ \t]|)[ \t]*$" , "%s=%s"],
      ],
    ],
    ]
  )
)
