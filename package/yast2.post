NAME="/etc/YaST2/ProductFeatures"
TEMPLATE="/var/adm/fillup-templates/YaST2.ProductFeatures"

if [ -x bin/fillup ] ; then
    if [ -f $TEMPLATE ] ; then
        echo "Updating $NAME..."
        test -f $NAME || touch $NAME
        bin/fillup -q $NAME $TEMPLATE
    fi
else
    echo "ERROR: fillup not found. This should not happen. Please compare"
    echo "etc/YaST2/ProductFeatures and $TEMPLATE and"
    echo "update by hand."
fi

