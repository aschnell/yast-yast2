{
    include "testsuite.ycp";
    import "Service";

    map READ = $[];
    map EXEC = $[
	"target": $[
	    "bash": 0,
	    "bash_output": $["exit": 0, "stdout": "", "stderr" :""],
	    ],
	];

    READ = $[ "init" : $[ "scripts" : $[
		"exists" : true,
	] ] ];
