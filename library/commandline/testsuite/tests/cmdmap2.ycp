{

    include "testsuite.ycp";
    import "TypeRepository";
    import "CommandLine";

    textdomain "empty";

    // map must define reasonable id
    TEST(``(CommandLine::Init(
	$[
	    "id": "",
	    "help" : _("This is testsuite")
	]
	,[]) ),[],nil);
    TEST(``(CommandLine::Done()),[],nil);
    TEST(``(CommandLine::Aborted()),[],nil);

/* EOF */
}
