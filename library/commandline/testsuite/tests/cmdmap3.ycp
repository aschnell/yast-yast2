{

    include "testsuite.ycp";
    import "./../../types/src/TypeRepository.ycp";
    import "./../src/CommandLine.ycp";

    // map must define help for each action
    TEST(``(CommandLine::Init(
	$[
	    "id": "testsuite",
	    "actions": $[
		"okaction" : $[
		    "help" : "This action should be ok"
		],
		"wrongaction" : $[
		    "key"  : nil
		]
	    ]
	]
	,[]) ),[],nil);
    TEST(``(CommandLine::Done()),[],nil);
    TEST(``(CommandLine::Aborted()),[],nil);

/* EOF */
}
