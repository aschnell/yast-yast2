Write	.dev.tty.prompt "YaST2 lan> " true
Write	.dev.tty "\nConfiguration of network cards\n------------------------------\n" true
Write	.dev.tty "Command 'add'" true
Write	.dev.tty "    add a network card" true
Write	.dev.tty "\n    Options:" true
Write	.dev.tty "        device   [string]     device ID" true
Write	.dev.tty "        ip       [ipaddress]  device address" true
Write	.dev.tty "        netmask  [netmask]    network mask" true
Write	.dev.tty "        blem                  other argument (without spaces and '1')" true
Write	.dev.tty "        atboot   [ yes no ]   turning on the device at boot" true
Write	.dev.tty "        help                  print the help for this command" true
Write	.dev.tty "        verbose               show progress information" true
Write	.dev.tty "" true
Return	false
