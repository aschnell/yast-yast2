DUMP("String::CutBlanks");
TEST(``(String::CutBlanks(nil)), [], nil);
TEST(``(String::CutBlanks("")), [], nil);
TEST(``(String::CutBlanks(" ")), [], nil);

TEST(``(String::CutBlanks("abc")), [], nil);
TEST(``(String::CutBlanks(" abc")), [], nil);
TEST(``(String::CutBlanks("abc ")), [], nil);
TEST(``(String::CutBlanks(" abc ")), [], nil);
TEST(``(String::CutBlanks("  abc")), [], nil);
TEST(``(String::CutBlanks("abc  ")), [], nil);

TEST(``(String::CutBlanks("ab c")), [], nil);
TEST(``(String::CutBlanks(" ab c")), [], nil);
TEST(``(String::CutBlanks("ab c ")), [], nil);
TEST(``(String::CutBlanks(" ab c ")), [], nil);
TEST(``(String::CutBlanks("  ab c")), [], nil);
TEST(``(String::CutBlanks("ab c  ")), [], nil);
TEST(``(String::CutBlanks("  ab c  ")), [], nil);
TEST(``(String::CutBlanks("ab  c")), [], nil);

