{

    include "testsuite.ycp";
map READ = $[
  "target" : $[
    "tmpdir" : "/tmp"
  ]
];
TESTSUITE_INIT ([READ], nil);

    import "TypeRepository";

    TEST(``(TypeRepository::regex_validator( "^[abc]+.*m$", "blem" )),[],nil);
    TEST(``(TypeRepository::regex_validator( "^[abc]+.*m$", "blec" )),[],nil);

/* EOF */
}
