{
    module "Integer";
    textdomain "base";


    /**
     * Generate a list<integer> with the integers from 0 to n-1.
     */
    global list<integer> Range(integer n)
    {
	list<integer> ret = [];
	integer i = 0;
	while (i < n)
	{
	    ret = add(ret, i);
	    i = i + 1;
	}
	return ret;
    }
}
